<div class="row">
  <div class="col-md-12">
    <%= search_form_for @q do |f| %>
      <div class="col-md-3">
        <%= f.select :address_region_id_eq,
          options_from_collection_for_select(Region.all, "id", "name",
            @q.address_region_id_eq), {include_blank: true}, {class: "form-control"} %>
      </div>
      <div class="col-md-1">
        <%= f.submit "Seach", class: "btn btn-default" %>
      </div>
    <% end %>
  </div>
</div>
